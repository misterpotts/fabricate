    "fs-extra": "^11.1.1",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
