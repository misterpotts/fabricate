    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.191",
    "@types/sinon": "^10.0.13",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "sinon": "^14.0.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.1"
